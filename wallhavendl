#!/usr/bin/env python

import os
import sys
from dataclasses import dataclass


@dataclass
class Config:
    wallh_api_key: str
    wallh_api_url: str
    wallh_categories: int
    wallh_purity: int
    wallh_colors: str


def set_defaults():
    Config.wallh_api_key = ''
    Config.wallh_api_url = ''
    Config.wallh_categories = 110
    Config.wallh_purity = 110
    Config.wallh_colors = ''


def main():
    set_defaults()


if __name__ == "__main__":
    main()
