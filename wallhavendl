#!/usr/bin/env python

# You can read about the parameters here:
# https://wallhaven.cc/help/api#search

import os
import sys
from dataclasses import dataclass


@dataclass
class Config:
    wallh_api_key: str
    wallh_api_url: str
    wallh_categories: int
    wallh_purity: int
    wallh_colors: str


def set_defaults():
    # remember to remove api key!!!!!!
    Config.wallh_api_key = 'F3HZhKeYKmuRZqqLqBySt8NUTf0GxGr1'
    Config.wallh_api_url = ''
    Config.wallh_categories = 110
    Config.wallh_purity = 110
    Config.wallh_colors = ''


def main():
    set_defaults()


if __name__ == "__main__":
    main()
